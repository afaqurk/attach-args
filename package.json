{
  "name": "arguments-to-class-properties",
  "version": "1.0.0",
  "description": "Utility to attach arguments to the constructor of a JavaScript class as named class properties",
  "main": "./lib/index.js",
  "scripts": {
    "pack": "webpack ./lib/index.js --output-library attachArgsToClass --output-filename attachArgsToClass.js --output-path ./dist/",
    "develop": "webpack --watch --colors --progress ./lib/index.js --output-library attachArgsToClass --output-filename attachArgsToClass.js --output-path ./dist/",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afaqurk/arguments-to-class-properties.git"
  },
  "author": "Afaq Tariq <afaq.tariq@inmar.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afaqurk/arguments-to-class-properties/issues"
  },
  "homepage": "https://github.com/afaqurk/arguments-to-class-properties#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "webpack": "^1.13.0"
  }
}
